<launch>
    <arg name="des_x" />
    <arg name="des_y" />
    <param name="des_pos_x" value="$(arg des_x)" />
    <param name="des_pos_y" value="$(arg des_y)" />
    <node pkg="motion_plan" type="wall_follow_service.py" name="wall_follower" output="screen" />
    <node pkg="motion_plan" type="go_to_point_service.py" name="go_to_point" output="screen" />
    <node pkg="motion_plan" type="bug.py" name="bug" output="screen" />
</launch>